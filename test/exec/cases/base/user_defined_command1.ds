
# exit status

hoge() {
    echo hello
}

hoge
assert($? == 0)

hoge2() {
   false
   return 34
}

hoge2
assert($? == 34)

hoge3() {
    assert($false)
}

hoge3
assert($? != 0)

hoge4() {
    exit 6
}

hoge4
assert($? == 6)

# argument
echo2() {
    echo $@
}

assert("$(echo2 a b c)" == "a b c")


# command override
## override builtin
cd() {
    echo hello
}

assert("$(cd)" == "hello")

## override external
ls() {
    command ls $@
}

assert("$(ls)" == "$(command ls)")
true    # dummy
