function func($num: Int) : Boolean {
	if ($num == 2) {
		return $true
	}
	return $false
}

assert not $func(1)
assert($func(2))
assert(not $func(3))

assert if $true { $true; } else { $false; }
assert if $false { throw 34; } else { $true; }

var i = 0
if $true { $i = 1; $i; } else { $i = 100; $true; }
assert $i == 1