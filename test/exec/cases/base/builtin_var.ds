# for builtin variable or constant

assert($YDSH_VERSION == "$(ps_intrp '\V')")

# $, PID, PPID
assert $$ == $PID
assert $PID != $PPID

child() {
    return $$ == $PPID && $$ != $PID ? 0 : 1
}

assert child

assert $HISTCMD == 1u    # HISTCMD is always 1 in scripting mode.

# UID, EUID
assert $UID == $EUID
assert $UID as String == "$(id -u)"

# uname
assert($OSTYPE == "$(uname -s)")
assert $MACHTYPE == "$(uname -m)"

# random number, at least 32767 (RAND_MAX)
{
    var r = $RANDOM
    assert $r != $RANDOM    # value of $RANDOM is random number
}