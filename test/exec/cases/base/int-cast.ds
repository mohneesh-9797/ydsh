
###############
#     Int     #
###############

var i64 = 1030792151049
assert($i64 is Int)

# to Float
assert($i64 as Float is Float)
assert($i64 as Float as String == '1030792151049.000000')
assert($i64.toFloat() as String == '1030792151049.000000')
$i64 = 7865
assert($i64 as Float as String == '7865.000000')
assert($i64.toFloat() as String == '7865.000000')
$i64 = -12344
assert($i64 as Float as String == '-12344.000000')
assert($i64.toFloat() as String == '-12344.000000')


#################
#     Float     #
#################

var f = -1.0
assert($f is Float)

# to Int
assert($f as Int is Int)
assert($f as Int as String == '-1')
assert($f.toInt() as String == '-1')
$f = 65535.9234
assert($f as Int as String == '65535')
assert($f.toInt() as String == '65535')

# to Float
assert($f as Float is Float)
assert($f as Float as String == '65535.923400')