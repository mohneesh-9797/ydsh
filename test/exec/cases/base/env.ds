import-env HOME

assert($HOME is String)
assert($HOME == "$(echo ~)")

# illegal env
try {
    import-env hferur
    assert($false)
} catch($e : Error) {
    assert($e is SystemError)
}

# default value
import-env zaswd :
    "default env"
assert($zaswd == "default env")


export-env ZZZ = $HOME
assert($ZZZ is String)
assert($ZZZ == $HOME)
$ZZZ = "hey"
assert($ZZZ == "hey")




