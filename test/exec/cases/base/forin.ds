# Array for-in
var a = [1, 2, 3, 5, 7]
var i = 0

for $val in $a {
    assert($val is Int)
	assert($val == $a[$i])
	$i = $i + 1
}
assert($i == 5)


# Map for-in
var b = ["a" : 1,
            "b" : 2,
            "c" : 3,
            "d" : 4]

$i = 0;
for $e in $b {
    assert($e is Tuple<String, Int>)
    assert($b[$e._0] == $e._1)
    $i++
}
assert($i == 4)

# String for-in
var r = ['あ', '0', '漢', '　']
$i = 0
for $a in "あ0漢　" {
    assert($a == $r[$i])
    $i++
}
assert($i == $r.size())