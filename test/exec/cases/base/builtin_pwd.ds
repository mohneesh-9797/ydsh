
# meaningless argument
assert pwd johefre

cd
assert($PWD == "$(pwd)")

# error (not found)
let tmp_dir = "$(mktemp -d 2> /dev/null || mktemp -d -t lfreop)"


assert(cd $tmp_dir)
assert(rm -rf $tmp_dir)

pwd # error
assert($? != 0)

# invalid option
assert(pwd --- 2>&1 | grep '\-ydsh: pwd: ---: invalid option')

# help
help pwd | grep "pwd: pwd \[-LP]"
assert($? == 0)
