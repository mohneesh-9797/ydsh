function func($x: Int) : Int {
	return $x * 2
}

function call() {
	var sub = $func
	assert($func(3) == 6)
	assert($sub(3) == 6)
}

$call()

function f($obj :
            Func<
               Int,
               [Int]>,
          $value : Int) : Int {
	return $obj($value)
}

assert($f($func, 12) == 24)